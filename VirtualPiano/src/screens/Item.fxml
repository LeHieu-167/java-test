<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane style="-fx-background-color: #5dacd9;" styleClass="stack-pane" stylesheets="@helpBackground.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefHeight="175.0" prefWidth="305.0">
         <children>
            <Label fx:id="lblTitle" alignment="CENTER" prefHeight="50.0" prefWidth="305.0" text="Title" textFill="#eaf1f4">
               <font>
                  <Font name=".VnBlack" size="17.0" />
               </font></Label>
            <Label fx:id="lblCost" prefHeight="50.0" prefWidth="305.0" textFill="WHITE" />
            <Button fx:id="btnPlay" mnemonicParsing="false" onAction="#btnPlayClicked" prefHeight="50.0" prefWidth="109.0" style="-fx-background-color: transpare; -fx-border-color: white; -fx-border-width: 5;" text="Play" textFill="#fafafa" />
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="305.0" spacing="20.0" />
   </children>
</AnchorPane>
